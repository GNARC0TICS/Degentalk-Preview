{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm run kill-ports && concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:seed": "npm run kill-ports && npm run seed:all && concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:quick": "npm run kill-ports && concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend:quick\" \"npm run dev:frontend\"",
    "dev:frontend": "vite --port 5173 --strictPort --config config/vite.config.ts",
    "dev:backend": "tsx --watch -r tsconfig-paths/register server/index.ts",
    "dev:backend:seed": "npm run seed:all && tsx --watch -r tsconfig-paths/register server/index.ts",
    "dev:backend:quick": "QUICK_MODE=true tsx --watch -r tsconfig-paths/register server/index.ts",
    "kill-ports": "npm run kill-port:5001 && npm run kill-port:5173",
    "kill-port:5001": "lsof -ti:5001 | xargs kill -9 2>/dev/null || echo \"Port 5001 clear\"",
    "kill-port:5173": "lsof -ti:5173 | xargs kill -9 2>/dev/null || echo \"Port 5173 clear\"",
    "build": "tsc && vite build --config config/vite.config.ts",
    "start": "NODE_ENV=production tsx -r tsconfig-paths/register server/index.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "npx dotenv-cli -e env.local -- drizzle-kit generate",
    "db:migrate:apply": "npx dotenv-cli -e env.local -- drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio",
    "db:create-tables": "npx dotenv-cli -e env.local -- npx tsx -r tsconfig-paths/register scripts/db/create-missing-tables.ts",
    "db:seed-chat": "npx dotenv-cli -e env.local -- npx tsx -r tsconfig-paths/register scripts/db/seed-chat.ts",
    "seed:all": "npm run db:migrate:apply && npm run seed:users && npm run seed:forum && npm run seed:threads && npm run seed:xp && npm run seed:levels && npm run seed:economy",
    "reset-and-seed": "npx dotenv-cli -e env.local -- npx tsx scripts/db/reset-and-seed.ts",
    "seed:users": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-users.ts",
    "seed:forum": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-canonical-zones.ts",
    "seed:forum:setup": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/setup-forum-structure.ts",
    "seed:threads": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-threads.ts",
    "seed:xp": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-xp-actions.ts",
    "seed:levels": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-default-levels.ts",
    "seed:economy": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-economy-settings.ts",
    "init:xp": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/initialize-xp-system.ts",
    "test:xp": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/testing/test-xp-system.ts",
    "check:imports": "tsx scripts/tools/check-imports.ts",
    "fix:imports": "tsx scripts/tools/check-imports.ts --fix",
    "generate:tree": "node scripts/tools/generate-tree.js > directory-tree.md",
    "validate:routes": "node scripts/testing/validate-routes.js",
    "test:routes:mock": "node --experimental-modules scripts/testing/test-mock-requests.js",
    "test:domain-migration": "./scripts/testing/validate-domain-migration.sh",
    "test:forum-endpoints": "node scripts/testing/test-forum-endpoints.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup:canonical-zones": "npm run db:migrate && tsx scripts/db/seed-canonical-zones.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
