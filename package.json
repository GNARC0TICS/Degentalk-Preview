{
	"name": "degentalk-landing",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@9.15.0",
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=9.0.0"
	},
	"scripts": {
		"dev": "cd client && pnpm dev",
		"build": "cd client && pnpm build",
		"preview": "cd client && pnpm preview",
		"clean": "pnpm -r exec rm -rf node_modules dist build && rm -rf node_modules",
		"install:clean": "pnpm clean && pnpm install"
	},
	"devDependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/typography": "^0.5.15",
		"@types/node": "^20.19.7",
		"@types/react": "^18.3.20",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.0",
		"postcss": "^8.4.47",
		"prettier": "^3.5.3",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "5.6.3",
		"uuid": "^11.1.0"
	},
	"pnpm": {
		"overrides": {
			"typescript": "5.6.3"
		},
		"peerDependencyRules": {
			"allowedVersions": {
				"react": "18",
				"react-dom": "18"
			}
		}
	},
	"dependencies": {
		"zod": "^3.24.2"
	}
}
