{
	"name": "rest-express",
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "npm run kill-ports && concurrently \"npm run dev:server\" \"npm run dev:client\" --names \"backend,frontend\" --prefix-colors \"cyan,yellow\"",
		"test:repo": "vitest -c server/vitest.config.ts",
		"sc": "superclaude",
		"cursor:setup": "scripts/dev/cursor-sync.sh",
		"dev:client": "vite --force --port 5173 --config config/vite.config.ts",
		"dev:server": "cd server && npm run dev",
		"dev:seed": "npm run kill-ports && npm run seed:all && concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend\" \"npm run dev:client\"",
		"dev:quick": "npm run kill-ports && concurrently --names \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend:quick\" \"npm run dev:client\"",
		"dev:backend": "npm run kill-port:5001 && tsx --tsconfig tsconfig.json --require tsconfig-paths/register server/index.ts",
		"dev:backend:seed": "npm run seed:all && tsx --watch -r tsconfig-paths/register server/index.ts",
		"dev:backend:quick": "QUICK_MODE=true tsx --watch -r tsconfig-paths/register server/index.ts",
		"kill-ports": "npm run kill-port:5001 && npm run kill-port:5173",
		"kill-port:5001": "lsof -ti:5001 | xargs kill -9 2>/dev/null || echo \"Port 5001 clear\"",
		"kill-port:5173": "lsof -ti:5173 | xargs kill -9 2>/dev/null || echo \"Port 5173 clear\"",
		"build": "npm run build:client && npm run build:server",
		"build:client": "vite build --config config/vite.config.ts",
		"build:server": "cd server && npm run build",
		"start": "cd server && npm start",
		"db:push": "drizzle-kit push",
		"db:migrate": "npx dotenv-cli -e env.local -- drizzle-kit generate",
		"db:migrate:Apply": "npx dotenv-cli -e env.local -- drizzle-kit push",
		"db:drop": "drizzle-kit drop",
		"db:studio": "drizzle-kit studio",
		"db:introspect": "npx dotenv-cli -e env.local -- drizzle-kit pull",
		"db:baseline": "npm run db:introspect && git add -A migrations/postgres/schema.ts migrations/postgres/relations.ts && git commit -m \"chore(db): baseline schema snapshot\" --no-verify || echo 'No changes to baseline'",
		"db:drift": "npm run db:introspect && git ls-files --error-unmatch migrations/postgres/schema.ts > /dev/null 2>&1 && git diff --exit-code migrations/postgres/schema.ts || (echo '⚠ Schema drift detected — review diff above.' && exit 1)",
		"db:create-tables": "npx dotenv-cli -e env.local -- npx tsx -r tsconfig-paths/register scripts/db/create-missing-tables.ts",
		"db:seed-chat": "npx dotenv-cli -e env.local -- npx tsx -r tsconfig-paths/register scripts/db/seed-chat.ts",
		"seed:all": "npm run db:migrate:Apply && npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/seed/seed-all-comprehensive.ts",
		"seed:wipe": "npm run seed:all -- --wipe",
		"seed:forums:only": "npm run seed:all -- --forums-only",
		"seed:users": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-users.ts",
		"seed:dev-wallet": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-dev-wallet.ts",
		"seed:dev-subscriptions": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-dev-subscriptions.ts",
		"seed:dev-complete": "npm run seed:users && npm run seed:dev-wallet && npm run seed:dev-subscriptions",
		"seed:threads": "npm run seed:all",
		"seed:achievements": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/seed-achievements.ts",
		"sync:forums": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/dev/syncForumsToDB.ts",
		"sync:check-forum-config": "npx dotenv-cli -e env.local -- tsx scripts/ops/check-forum-config-sync.ts",
		"dev:fresh-wallet": "npx dotenv-cli -e env.local -- tsx scripts/dev/setup-fresh-wallet-test.ts",
		"init:xp": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/db/initialize-xp-system.ts",
		"test:xp": "npx dotenv-cli -e env.local -- tsx -r tsconfig-paths/register scripts/testing/test-xp-system.ts",
		"generate:tree": "node scripts/tools/generate-tree.js > directory-tree.md",
		"generate:types": "drizzle-kit generate:pg --schema=./db/schema --out=./db/types/generated",
		"build:forum-sdk": "tsx scripts/build-forum-sdk.ts",
		"validate:routes": "node scripts/testing/validate-routes.js",
		"test:routes:mock": "node --experimental-modules scripts/testing/test-mock-requests.js",
		"test:domain-migration": "./scripts/testing/validate-domain-migration.sh",
		"test:forum-endpoints": "node scripts/testing/test-forum-endpoints.js",
		"test:forum-fks": "tsx scripts/testing/validate-forum-fks.ts",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:unit": "vitest --config client/vitest.config.ts",
		"lint": "npm run lint:client && npm run lint:server",
		"lint:client": "eslint client --ext .ts,.tsx --max-warnings 0",
		"lint:server": "cd server && npm run lint",
		"preview": "vite preview",
		"setup:canonical-zones": "npm run db:migrate && tsx scripts/db/seed-canonical-zones.ts",
		"test": "npm run test:client && npm run test:server",
		"test:client": "cd client && vitest --config vitest.config.ts",
		"test:client:watch": "vitest --watch --config client/vitest.config.ts",
		"test:server": "cd server && npm test",
		"seed:ui-quotes": "tsx scripts/db/seed-ui-config-quotes.ts",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"validate": "tsx scripts/validate-everything.ts",
		"validate:admin": "npx tsx scripts/admin/validate-admin-controllers.ts",
		"validate:fix": "tsx scripts/validate-everything.ts --fix",
		"check": "tsx scripts/test-ci-readiness.ts",
		"precommit": "npm run validate",
		"prebuild": "",
		"mcp:neon": "npx @neondatabase/mcp-server-neon start napi_64010heeqqly1w5t5eg1dpjposfjcqpdg0lu4bc7yvnci0vh7qyivoo04i3j036x",
		"spawn": "tsx scripts/ops/neon-sync-agent.ts",
		"migration:validate": "tsx scripts/ops/validate-safe-migrations.ts validate",
		"migration:template": "tsx scripts/ops/validate-safe-migrations.ts template",
		"typecheck": "tsc --noEmit",
		"typecheck:watch": "tsc --noEmit --watch",
		"quality:check": "tsx scripts/quality/quality-metrics.ts",
		"tokens": "style-dictionary build",
		"prepare": "simple-git-hooks",
		"codemod:dry-run": "bash scripts/refactor/component-merge/run-dry-run.sh",
		"generate:scope": "tsx scripts/dev/generate-scope-guide.ts",
		"scan:icons": "tsx scripts/tools/icon-scan.ts",
		"db:reset-hard": "npx dotenv-cli -e env.local -- psql $DATABASE_URL -c \"DROP SCHEMA IF EXISTS public CASCADE; CREATE SCHEMA public; DROP SCHEMA IF EXISTS drizzle CASCADE;\" && npm run db:migrate:Apply",
		"docs:frontmatter": "node scripts/docs/add-frontmatter.cjs",
		"docs:lint": "markdownlint docs/**/*.md && markdown-link-check -q -c .markdown-link-check.json docs/**/*.md"
	},
