{
  "framework": "vite",
  "buildCommand": "cd client && pnpm build",
  "outputDirectory": "client/dist",
  "installCommand": "pnpm install",
  "devCommand": "cd client && pnpm dev",
  "env": {
    "VITE_GA_TRACKING_ID": "@vite_ga_tracking_id"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18",
      "VITE_GA_TRACKING_ID": "@vite_ga_tracking_id"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ]
}